# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password_here

# n8n Configuration
N8N_USER=admin
N8N_PASSWORD=your_secure_n8n_password_here
N8N_ENCRYPTION_KEY=your_32_character_encryption_key_here

# Notion API Configuration
NOTION_API_KEY=your_notion_integration_token_here
NOTION_DATABASE_ID_TASKS=your_tasks_database_id_here
NOTION_DATABASE_ID_MATERIALS=your_materials_database_id_here
NOTION_DATABASE_ID_RISKS=your_risks_database_id_here
NOTION_DATABASE_ID_TEAM=your_team_database_id_here

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama2:13b
EMBEDDING_MODEL=nomic-embed-text:latest

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=materials

# Image Tags (Version Pinning)
POSTGRES_TAG=15-alpine
REDIS_TAG=7.2-alpine
N8N_TAG=1.76.0
OLLAMA_TAG=0.3.12
QDRANT_TAG=v1.8.4

# Application Configuration
APP_ENV=development
APP_DEBUG=true
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_here
